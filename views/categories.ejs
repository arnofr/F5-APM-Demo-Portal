<div layout="row" flex layout-sm="column">
  <div flex="nogrow">
    <md-card >
      <md-toolbar class="md-table-toolbar md-default" >
        <div class="md-toolbar-tools">
          <span>Category List</span>
        </div>
      </md-toolbar>
      <md-table-container >
        <table md-table   md-progress="promise">
          <thead md-head md-order="query.order" md-on-reorder="">
            <tr md-row>
              <th md-column md-numeric>#</th>
              <th md-column md-order-by="nameToLower">Categories</th>
            </tr>
          </thead>
          <tbody md-body>
            <tr md-row  ng-repeat="urlcategory in urlcategories">
               <td md-cell>{{$index+1}}</td>
               <td md-cell><md-button class="md-primary" href='#/category/urlcategories/{{$index}}'>{{urlcategory.name}}</md-button></td>
             </tr>
           </tbody>
        </table>
      </md-table-container>
    </md-card>
  </div>
  <div ui-view layout=row></div>
<div>
